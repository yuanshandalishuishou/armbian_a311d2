CONFIG_RTL8192EU=m
CONFIG_RTL8192SU=m
CONFIG_RTL8723AU=m
CONFIG_RTL8723BU=m
CONFIG_RTL8821AE=m
CONFIG_RTL8821CE=m
CONFIG_RTL8822BE=m
CONFIG_RTL8822CE=m

# Atheros 无线支持
CONFIG_WLAN_VENDOR_ATH=y
CONFIG_ATH9K_HTC=m
CONFIG_ATH10K=m
CONFIG_ATH10K_PCI=m
CONFIG_ATH10K_USB=m

# MediaTek 无线支持
CONFIG_WLAN_VENDOR_MEDIATEK=y
CONFIG_MT76_CORE=m
CONFIG_MT76_USB=m
CONFIG_MT76x2U=m
CONFIG_MT7921U=m

# 蓝牙增强支持
CONFIG_BT_HCIBTUSB=m
CONFIG_BT_HCIUART=m
CONFIG_BT_HCIUART_BCSP=y
CONFIG_BT_HCIUART_ATH3K=y
CONFIG_BT_HCIUART_3WIRE=y
CONFIG_BT_RFCOMM=m
